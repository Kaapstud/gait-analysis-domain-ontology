    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Foot_pronation -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Foot_pronation">
        <rdf:type rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_Measurement_Point"/>
