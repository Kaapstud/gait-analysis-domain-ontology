        <rdfs:label xml:lang="en">hasGait_phase_cycle</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#hasGait_type -->

