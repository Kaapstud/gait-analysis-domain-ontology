        <Gait_Ontology:hasPathology_trait rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reduced rang of motion in foot.</Gait_Ontology:hasPathology_trait>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Healthcare_practitioner -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Healthcare_practitioner">
        <rdf:type rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_expert"/>
        <Gait_Ontology:administers rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_assessment"/>
        <Gait_Ontology:analyzes rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Clinical_data"/>
        <Gait_Ontology:facilitates rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Clinical_evaluation"/>
        <Gait_Ontology:observes rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_walk"/>
        <Gait_Ontology:prescribes rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Walker"/>
        <Gait_Ontology:treats rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Elderly"/>
        <Gait_Ontology:hasGait_expert rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Healthcare practitioner</Gait_Ontology:hasGait_expert>
        <Gait_Ontology:hasGender rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Male</Gait_Ontology:hasGender>
        <rdfs:comment xml:lang="en">A healthcare practitioner is a person who is trained and licensed to provide medical treatment to individuals. This can include doctors, nurses, surgeons, and other professionals who are qualified to diagnose and treat patients. Healthcare practitioners typically work in a variety of settings, such as hospitals, clinics, private practices, and other healthcare facilities. They may also work in specialised areas of medicine, such as pediatrics, surgery, or mental health. The specific duties and responsibilities of a healthcare practitioner will depend on their specific area of practice and the needs of their patients.</rdfs:comment>
        <rdfs:label xml:lang="en">Healthcare_practitioner</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Hip_abductor_weakness -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Hip_abductor_weakness">
