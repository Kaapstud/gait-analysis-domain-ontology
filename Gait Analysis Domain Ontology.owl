    <owl:Class rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Neuromuscular">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_Classification"/>
