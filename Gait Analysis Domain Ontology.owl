        <rdfs:comment xml:lang="en">Observing the length of the stride, arm movement, heel placement and angle of the pelvis and foot.</rdfs:comment>
        <rdfs:label xml:lang="en">hasMeasurement</rdfs:label>
    </owl:DatatypeProperty>
    


