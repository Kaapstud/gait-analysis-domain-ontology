        <Gait_Ontology:collects rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Clinical_data"/>
        <Gait_Ontology:involves rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Adolescent"/>
        <Gait_Ontology:involves rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Adult"/>
        <Gait_Ontology:involves rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Athlete"/>
        <Gait_Ontology:involves rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Child"/>
        <Gait_Ontology:involves rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Elderly"/>
        <Gait_Ontology:isPartOf rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Clinical_evaluation"/>
        <Gait_Ontology:hasGait_acquisition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gait (data) aquisition</Gait_Ontology:hasGait_acquisition>
        <rdfs:comment xml:lang="en">Gait acquisition is the process of collecting and analyzing data on an individual&apos;s gait, or the way in which they walk.

Gait (data) acquisition is the process where a patient&apos;s gait assessment  or measurements are obtained in acquisition sessions and observations for quantitative recording and functional description.</rdfs:comment>
        <rdfs:label xml:lang="en">Session</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Severe -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Severe">
