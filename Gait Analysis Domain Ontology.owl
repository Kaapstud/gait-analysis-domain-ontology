    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_training -->

    <owl:Class rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_training">
