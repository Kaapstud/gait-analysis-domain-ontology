    </owl:Class>
    


    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Difficulty_walking_backward -->

    <owl:Class rdf:about="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Difficulty_walking_backward">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_Disturbance"/>
        <opla-sd:entityPosition>
            <rdf:Description>
                <opla-sd:entityPositionX rdf:datatype="http://www.w3.org/2001/XMLSchema#double">132.24248061097418</opla-sd:entityPositionX>
                <opla-sd:entityPositionY rdf:datatype="http://www.w3.org/2001/XMLSchema#double">230.3087226962265</opla-sd:entityPositionY>
