        <Gait_Ontology:isEvaluatedIn rdf:resource="http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#Gait_walk"/>
        <Gait_Ontology:hasParameter rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tempo</Gait_Ontology:hasParameter>
        <rdfs:comment xml:lang="en">Pace - speed in walking, running, or moving.
Pace - the speed or rate at which something happens or develops.
Pace - a unit of length representing the distance between two successive steps in walking.</rdfs:comment>
        <rdfs:label xml:lang="en">Pace</rdfs:label>
    </owl:NamedIndividual>
    


