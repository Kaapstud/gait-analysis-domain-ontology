        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The occupational title of the gait (analysis) expert.</rdfs:comment>
        <rdfs:label xml:lang="en">hasGait_Expert</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/marthinust/ontologies/2022/8/Gait_Ontology#hasGait_phase_cycle -->

